
	public class Student extends Practice {

	    private int id;
	    
	    public int getId() {
	        return id;
	    }

	    public void setId(int id) {
	        this.id = id;
	    }

	    void display() {
	        System.out.println("Name: " + getName());
	        System.out.println("Age: " + getAge());
	        System.out.println("Id: " + getId());
	    }

	    }

	
	
