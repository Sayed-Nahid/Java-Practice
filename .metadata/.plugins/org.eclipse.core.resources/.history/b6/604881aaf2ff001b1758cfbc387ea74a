
public class MainClass {
	public static void main(String[] args)
	{
		
	}
}



//First class

class P1

{

 void method1()

 {

     System.out.println("P1");

 }

}

//Second Class

class P2

{

 void method1()

 {

     System.out.println("P2");

 }

} 

//Main Class

class TestClass extends P1, P2

{

public static void main(String args[])

{

    TestClass ob = new TestClass();

    ob.method1();

}

}